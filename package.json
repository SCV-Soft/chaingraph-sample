{
  "name": "sample",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "create-graph": "graph create --node https://klaytn-node.graph.scv.sh/deploy your_project_name/your_graph_name --access-token your_access_token",
    "deploy": "graph deploy --ipfs https://ipfs.graph.scv.sh --node https://klaytn-node.graph.scv.sh/deploy your_project_name/your_graph_name --deploy-key your_access_token",
    "create-local": "graph create --node http://localhost:8020/ your_project_name/your_graph_name",
    "remove-local": "graph remove --node http://localhost:8020/ your_project_name/your_graph_name",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 your_project_name/your_graph_name"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.33.0",
    "@graphprotocol/graph-ts": "^0.25.0"
  }
}
